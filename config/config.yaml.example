server:
  http_port: 48891
  ws_port: 48892
  bind_ip: "0.0.0.0"

ssh_server:
  host: "YOUR_SSH_HOST"
  port: 22
  user: "YOUR_SSH_USER"
  password: "YOUR_SSH_PASSWORD"
  key_file: ""

ftp_server:
  host: "YOUR_FTP_HOST"
  port: 21
  user: "YOUR_FTP_USER"
  password: "YOUR_FTP_PASSWORD"

log:
  level: "debug"
  file: "config/server.log"
